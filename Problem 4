n1 <- 100:999
n2 <- 100:999

res <- c()

for(i in n1){
  r <- i*n2
  res <- c(res,r)
}

is.palindrome <- function (word) {
  identical(word, paste(rev(strsplit(word, "")[[1]]), collapse=""))
}


raw <- c()
for(i in res){
    word <- as.character(i)
    if(is.palindrome(word)){
        raw <- c(raw,i)
   	
    }
}

raw <- sort(raw)

raw[length(raw)]
